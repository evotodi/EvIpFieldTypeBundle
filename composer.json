{
    "name": "evotodi/ip-field-symfony3-bundle",
    "type": "symfony-bundle",
    "description": "Ip form type for symfony 3 forms",
    "keywords": ["ip", "form", "field", "type", "symfony", "symfony bundle", "bundle", "symfony-3"],
    "homepage": "https://github.com/evotodi/EvIpFieldTypeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Davis",
            "email": "evotodi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/symfony": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": { "Evotodi\\IpFieldTypeBundle\\": "src/" }
    },
    "extra": {
        "symfony-assets-install":"symlink"
    }
